datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["interactiveTransactions"]
}

model User {
  id                  Int         @id @default(autoincrement())
  email               String      @unique
  name                String?
  hashedPass          String
  token               String?
  invalid_after       DateTime?
}
