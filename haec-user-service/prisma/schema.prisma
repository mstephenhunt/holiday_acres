datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["interactiveTransactions"]
}

model User {
  id           Int     @id @default(autoincrement())
  email        String  @unique
  name         String?
  hashed_pass  String
}
